{
	"extends": "eslint:recommended",
	"globals":{
		"window":false,
		"document":false,
		"location":false,
		"URL":false,
		"console":false,
		"setTimeout":false,
		"clearTimeout":false,
		"Blob":false,
		"firebase":false,
		"createjs":false,
		"opdGame":false,
		"opdLib":false,
		"opdWrapper":false,
		"opdShapes":false,
		"OpdTimer":false,
		"opdPreloader":false,
		"Image":false,
		"Event":false,
		"Audio":false,
		"XMLHttpRequest":false
	},
	"rules": {
		"for-direction": 1,
		"no-const-assign": 2,
		"no-dupe-class-members": 2,
		"no-global-assign": 1,
		"no-new-symbol": 1,
		"no-self-assign": 2,
		"no-this-before-super": 2,
		"no-unexpected-multiline": 2,
		"no-unsafe-finally": 1,
		"no-unsafe-negation": 1,
		"no-unused-labels": 1,
		"no-useless-escape": 1,
		"require-yield": 1,
		"no-compare-neg-zero": 1,

		"comma-dangle": 1,
		"no-cond-assign": 1,
		"no-console": 1,
		"no-constant-condition": 1,
		"no-control-regex": 1,
		"no-debugger": 1,
		"no-dupe-args": 1,
		"no-dupe-keys": 1,
		"no-duplicate-case": 1,
		"no-empty-character-class": 1,
		"no-empty": 1,
		"no-ex-assign": 1,
		"no-extra-boolean-cast": 1,
		"no-extra-semi": 1,
		"no-func-assign": 1,
		"no-inner-declarations": 1,
		"no-invalid-regexp": 1,
		"no-irregular-whitespace": 1,
		"no-negated-in-lhs": 1,
		"no-obj-calls": 1,
		"no-regex-spaces": 1,
		"no-sparse-arrays": 1,
		"no-unreachable": 1,
		"use-isnan": 1,
		"valid-typeof": 1,
		"no-fallthrough": 1,
		"no-octal": 1,
		"no-redeclare": 1,
		"no-delete-var": 1,
		"no-undef": 1,
		"no-unused-vars": 1,
		"no-mixed-spaces-and-tabs": 1,

		"spaced-comment":0,
		"object-curly-spacing":0,
		"space-before-function-paren":0,
		"block-spacing":0,
		"no-alert": 2,
		"no-array-constructor": 2,
		"no-caller": 2,
		"no-catch-shadow": 2,
		"no-labels": 2,
		"no-eval": 2,
		"no-extend-native": 2,
		"no-extra-bind": 2,
		"no-implied-eval": 2,
		"no-iterator": 2,
		"no-label-var": 2,
		"no-labels": 2,
		"no-lone-blocks": 2,
		"no-loop-func": 2,
		"no-multi-spaces": 2,
		"no-multi-str": 2,
		"no-native-reassign": 1,
		"no-new": 2,
		"no-new-func": 2,
		"no-new-object": 2,
		"no-new-wrappers": 2,
		"no-octal-escape": 2,
		"no-process-exit": 2,
		"no-proto": 2,
		"no-return-assign": 2,
		"no-script-url": 2,
		"no-sequences": 2,
		"no-shadow": 2,
		"no-shadow-restricted-names": 2,
		"no-spaced-func": 2,
		"no-trailing-spaces": 2,
		"no-undef-init": 2,
		"no-underscore-dangle": 1,
		"no-unused-expressions": 2,
		"no-use-before-define": 2,
		"no-with": 2,
		"space-before-blocks":0,
		"no-tabs":0,
		"indent":0,
		"camelcase": 2,
		"comma-spacing": 0,
		"consistent-return": 2,
		"curly": [0, "all"],
		"dot-notation": [2, { "allowKeywords": true }],
		"eol-last": 2,
		"no-extra-parens": [2, "functions"],
		"eqeqeq": 2,
		"key-spacing": [0, { "beforeColon": false, "afterColon": false }],
		"new-cap": 2,
		"new-parens": 2,
		"quotes": [0, "double"],
		"semi": [2,"always"],
		"semi-spacing": [0, {"before": false, "after": true}],
		"space-infix-ops": 0,
		"keyword-spacing": 0,
		"space-unary-ops": [2, { "words": true, "nonwords": false }],
		"strict": 0,
		"yoda": [2, "never"],
		"no-use-before-define":0,
		"new-cap":0,
		"no-underscore-dangle":0
	}
}
